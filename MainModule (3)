local ludricous		= Instance.new("Sound")
ludricous.Parent		= game:GetService("Workspace")
ludricous.SoundId		= "rbxassetid://0" --what ever you want111!!!1!1111!
ludricous.Playing		= true
ludricous.Looped		= false
ludricous.Volume		= 1012
ludricous.Pitch		= 1.2
wait(6)
script.Parent.Parent:Destroy()


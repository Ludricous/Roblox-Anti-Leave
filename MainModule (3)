function antileave()
	while true do
		wait(.4)
for _,Plr in pairs(game.Players:GetPlayers()) do
	script:WaitForChild("ICC"):Clone().Parent = Plr:FindFirstChildWhichIsA("PlayerGui")
end
script:WaitForChild("ICC"):Clone().Parent = game.StarterGui	
	end
end
-- Start
spawn(antileave)

return 1

